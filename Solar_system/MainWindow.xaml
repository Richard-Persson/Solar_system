        <Label Name="hastighetTekst" Content="Hastighet: 100" HorizontalAlignment="Left" Margin="314,293,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.066,0.136"/>
        <Button Content="-" HorizontalAlignment="Left" Margin="314,332,0,0" VerticalAlignment="Top" Click="Button_Click_Negative"/>
        <Button Content="+" HorizontalAlignment="Left" Margin="378,332,0,0" VerticalAlignment="Top" Click="Button_Click_Positive"/>
        <ComboBox HorizontalAlignment="Left" Margin="301,374,0,0" VerticalAlignment="Top" Width="120" Name="liste" SelectionChanged="liste_SelectionChanged">
            <ComboBoxItem Content="SolarSystem"/>
            <ComboBoxItem Content="Earth"/>
        </ComboBox>


        <Grid Name="SolarSystem">

            </Grid>
     


        <Viewbox Name="vb" Visibility="Hidden" Width="100" Height="100">
            <Grid Name="planet">
                
            </Grid>
        </Viewbox>
 
    </Grid>



</Window>
    
